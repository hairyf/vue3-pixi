{
  "name": "vue3-pixi-nuxt",
  "type": "module",
  "version": "1.0.0-beta.6",
  "description": "Nuxt createRenderer for PixiJS",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/vue3-pixi#readme",
  "repository": "vue3-pixi",
  "keywords": [
    "pixi.js",
    "vue"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@antfu/utils": "catalog:share",
    "@nuxt/kit": "catalog:nuxt",
    "@xmldom/xmldom": "catalog:nuxt",
    "mlly": "catalog:nuxt",
    "nuxi": "catalog:nuxt",
    "pkg-types": "catalog:nuxt",
    "sirv": "catalog:nuxt",
    "vue-demi": "catalog:share",
    "vue3-pixi": "workspace:^"
  },
  "devDependencies": {
    "@nuxt/module-builder": "catalog:nuxt",
    "@nuxt/schema": "catalog:nuxt",
    "@vue/runtime-core": "catalog:share",
    "nuxt": "catalog:nuxt"
  },
  "build": {
    "externals": [
      "node:child_process",
      "vite-plugin-glsl",
      "@rollup/pluginutils",
      "vite",
      "esbuild",
      "rollup",
      "estree-walker",
      "picomatch",
      "postcss",
      "lightningcss",
      "fsevents",
      "pnpapi",
      "picocolors",
      "source-map-js",
      "nanoid/non-secure"
    ]
  }
}
