{
  "name": "vue3-pixi-nuxt",
  "type": "module",
  "version": "0.9.3",
  "description": "Nuxt createRenderer for PixiJS",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/vue3-pixi#readme",
  "repository": "vue3-pixi",
  "keywords": [
    "pixi.js",
    "vue"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "dist/module.mjs",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.8",
    "@nuxt/kit": "^3.11.2",
    "@xmldom/xmldom": "^0.8.10",
    "mlly": "^1.7.0",
    "nuxi": "^3.12.0",
    "pkg-types": "^1.1.1",
    "sirv": "^2.0.4",
    "vue-demi": "^0.14.8",
    "vue3-pixi": "workspace:1.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.7.1",
    "@nuxt/schema": "^3.11.2",
    "@vue/runtime-core": "^3.4.27",
    "nuxt": "^3.11.2"
  },
  "build": {
    "externals": [
      "node:child_process",
      "vite-plugin-glsl",
      "@rollup/pluginutils",
      "vite",
      "esbuild",
      "rollup",
      "estree-walker",
      "picomatch",
      "postcss",
      "lightningcss",
      "fsevents",
      "pnpapi",
      "picocolors",
      "source-map-js",
      "nanoid/non-secure"
    ]
  }
}
